<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Calculate</servlet-name>
        <servlet-class>pl.polsl.lab.servlets.calculate.Calculate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>History</servlet-name>
        <servlet-class>pl.polsl.lab.servlets.history.History</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>pl.polsl.lab.servlets.register.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterAnswer</servlet-name>
        <servlet-class>pl.polsl.lab.servlets.register.RegisterAnswer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>pl.polsl.lab.servlets.login.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginAnswer</servlet-name>
        <servlet-class>pl.polsl.lab.servlets.login.LoginAnswer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainPage</servlet-name>
        <servlet-class>pl.polsl.lab.main.page.MainPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnRegister</servlet-name>
        <servlet-class>pl.polsl.lab.servlets.unregister.UnRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>pl.polsl.lab.servlets.logout.LogOut</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Calculate</servlet-name>
        <url-pattern>/Calculate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>History</servlet-name>
        <url-pattern>/History</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterAnswer</servlet-name>
        <url-pattern>/RegisterAnswer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginAnswer</servlet-name>
        <url-pattern>/LoginAnswer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainPage</servlet-name>
        <url-pattern>/MainPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnRegister</servlet-name>
        <url-pattern>/UnRegister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/LogOut</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            1
        </session-timeout>
    </session-config>
</web-app>
